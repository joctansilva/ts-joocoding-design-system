{
  "name": "@joocoding/monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=@joocoding/*",
    "prepack": "turbo run prepack",
    "postpack": "turbo run postpack",
    "dev": "turbo run dev --no-cache --continue",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:ui": "turbo run test:ui",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "clean:build": "turbo run clean:build",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "changeset:enter-alpha": "changeset pre enter alpha",
    "changeset:enter-beta": "changeset pre enter beta",
    "changeset:exit-alpha/beta": "changeset pre exit",
    "version-packages": "changeset version",
    "release": "turbo run release",
    "release:file": "turbo run release:file",
    "storybook": "storybook dev -p 6006",
    "clean:storybook": "rm -rf node_modules/.cache/storybook",
    "build:storybook": "storybook build",
    "serve:storybook": "serve -p 6006 storybook-static"
  },
  "devDependencies": {
    "@joocoding/tailwind": "workspace:*",
    "@joocoding/theme": "workspace:*",
    "@changesets/cli": "^2.25.2",
    "@chromatic-com/storybook": "^1.6.1",
    "@fontsource-variable/plus-jakarta-sans": "^5.0.6",
    "@storybook/addon-a11y": "^8.2.6",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/manager-api": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@storybook/theming": "^8.2.6",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.1",
    "@types/react": "^18",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "latest",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.1.0",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-preserve-directives": "^1.1.1",
    "serve": "^14.2.1",
    "storybook": "^8.2.6",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.9.3",
    "turbo": "latest",
    "typescript": "^5.4.3",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@storybook/test": "^8.2.6",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "tailwind-merge": "^2.2.2"
  }
}
